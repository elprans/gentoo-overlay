\|AC_SUBST(NSPR_CFLAGS)| a\
AC_DEFINE(NSPR_CFLAGS)

\|AC_SUBST(NSPR_LIBS)| a\
AC_DEFINE(NSPR_LIBS)

/if test -z "$MOZ_NATIVE_NSPR"; then/,+12 d
