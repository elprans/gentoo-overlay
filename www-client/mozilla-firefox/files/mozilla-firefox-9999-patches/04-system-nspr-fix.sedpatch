#@@configure.in

\|NSPR_CFLAGS='`$(DEPTH)/nsprpub/config/nspr-config --prefix=$(LIBXUL_DIST) --includedir=$(LIBXUL_DIST)/include/nspr --cflags`'| c\
NSPR_CFLAGS=`pkg-config mozilla-nspr --cflags`\
NSPR_LIBS=`pkg-config mozilla-nspr --libs`

\|NSPR_LIBS='`$(DEPTH)/nsprpub/config/nspr-config --prefix=$(LIBXUL_DIST) --libdir=$(LIBXUL_DIST)/lib --libs`'| c\
