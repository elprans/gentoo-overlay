From: Elvis Pranskevichus <elvis@alfheim.prans.org>
Date: Thu, 11 Dec 2008 21:08:16 -0500
Subject: [PATCH] Add ru: Yawerty layout variant

---
 rules/base.xml.in |    6 +++++
 symbols/ru        |   63 ++++++++++++++++++++++++++++++++++++++++++++++++++++-
 2 files changed, 68 insertions(+), 1 deletions(-)

diff --git a/rules/base.xml.in b/rules/base.xml.in
index 969906b..7e4f6a8 100644
--- a/rules/base.xml.in
+++ b/rules/base.xml.in
@@ -3430,6 +3430,12 @@
       <variantList>
         <variant>
           <configItem>
+            <name>yawerty</name>
+            <_description>Yawerty</_description>
+          </configItem>
+        </variant>
+        <variant>
+          <configItem>
             <name>phonetic</name>
             <_description>Phonetic</_description>
           </configItem>
diff --git a/symbols/ru b/symbols/ru
index 540e89e..e857803 100644
--- a/symbols/ru
+++ b/symbols/ru
@@ -8,8 +8,69 @@
 //
 // $XFree86: xc/programs/xkbcomp/symbols/ru,v 1.3 2003/02/15 22:27:05 dawes Exp $
 
-// Windows layout
 partial default alphanumeric_keys
+xkb_symbols "yawerty" {
+
+    name[Group1]= "Russia - Yawerty";
+
+    key	<AE01> {	[		1,	    exclam 	]	};
+    key	<AE02> {	[		2,        	at	]	};
+    key	<AE03> {	[		3,      numbersign 	]	};
+    key	<AE04> {	[		4,          dollar	]	};
+    key	<AE05> {	[		5,	   percent	]	};
+    key	<AE06> {	[		6,     asciicircum	]	};
+    key	<AE07> {	[		7,	 ampersand	]	};
+    key	<AE08> {	[		8,        asterisk	]	};
+    key	<AE09> {	[		9,	 parenleft	]	};
+    key	<AE10> {	[		0,	parenright	]	};
+
+    key	<AE11> {	[	    minus,	underscore	]	};
+    key	<AE12> {	[	    equal,	      plus	]	};
+
+    key	<AB09> {	[	   period,	   greater	]	};
+    key	<AB10> {	[	    slash,	  question	]	};
+    key	<AB08> {	[	    comma,	      less	]	};
+    key	<LSGT> {	[	      bar,	 brokenbar	]	};
+
+
+    key	<TLDE> {	[    Cyrillic_che,    Cyrillic_CHE,          grave, asciitilde	]	};
+    key	<LatQ> {	[     Cyrillic_ya,     Cyrillic_YA,		 q, Q		]	};
+    key	<LatZ> {	[     Cyrillic_ze,     Cyrillic_ZE,		 z, Z		]	};
+    key	<LatS> {	[     Cyrillic_es,     Cyrillic_ES,		 s, S		]	};
+    key	<LatA> {	[      Cyrillic_a,	Cyrillic_A,		 a, A		]	};
+    key	<LatW> {	[     Cyrillic_ve,     Cyrillic_VE,		 w, W		]	};
+    key	<LatC> {	[    Cyrillic_tse,    Cyrillic_TSE,              c, C		]	};
+    key	<LatX> {	[     Cyrillic_yu,     Cyrillic_YU,		 x, X		]	};
+    key	<LatD> {	[     Cyrillic_de,     Cyrillic_DE,		 d, D		]	};
+    key	<LatE> {	[     Cyrillic_ie,     Cyrillic_IE,		 e, E		]	};
+    key	<LatV> {	[      Cyrillic_e,      Cyrillic_E,		 v, V		]	};
+    key	<LatF> {	[     Cyrillic_ef,     Cyrillic_EF, 		 f, F		]	};
+    key	<LatT> {	[     Cyrillic_te,     Cyrillic_TE,		 t, T		]	};
+    key	<LatR> {	[     Cyrillic_er,     Cyrillic_ER,		 r, R		]	};
+    key	<LatN> {	[     Cyrillic_en,     Cyrillic_EN,		 n, N		]	};
+    key	<LatB> {	[     Cyrillic_be,     Cyrillic_BE,		 b, B		]	};
+    key	<LatH> {	[     Cyrillic_ha,     Cyrillic_HA,		 h, H		]	};
+    key	<LatG> {	[    Cyrillic_ghe,    Cyrillic_GHE,		 g, G		]	};
+    key	<LatY> {	[   Cyrillic_yeru,   Cyrillic_YERU,		 y, Y		]	};
+    key	<LatM> {	[     Cyrillic_em,     Cyrillic_EM,		 m, M		]	};
+    key	<LatJ> {	[ Cyrillic_shorti, Cyrillic_SHORTI,		 j, J		]	};
+    key	<LatU> {	[      Cyrillic_u,	Cyrillic_U,		 u, U		]	};
+    key	<LatK> {	[     Cyrillic_ka,     Cyrillic_KA,		 k, K		]	};
+    key	<LatI> {	[      Cyrillic_i,	Cyrillic_I,		 i, I		]	};
+    key	<LatO> {	[      Cyrillic_o,	Cyrillic_O,		 o, O		]	};
+    key	<LatL> {	[     Cyrillic_el,     Cyrillic_EL, 		 l, L		]	};
+    key	<LatP> {	[     Cyrillic_pe,     Cyrillic_PE,		 p, P		]	};
+    key	<AD11> {	[    Cyrillic_sha,    Cyrillic_SHA,    bracketleft, braceleft	]	};
+    key	<AD12> {	[  Cyrillic_shcha,  Cyrillic_SHCHA,   bracketright, braceright	]	};
+    key <AC10> {	[Cyrillic_softsign,Cyrillic_SOFTSIGN,    semicolon, colon	]	};
+    key <AC11> {	[    Cyrillic_zhe,    Cyrillic_ZHE,	apostrophe, quotedbl	]	};
+    key	<BKSL> {	[Cyrillic_hardsign,Cyrillic_HARDSIGN,	 backslash, bar		]	};
+
+    include "level3(ralt_switch)"
+};
+
+// Windows layout
+partial alphanumeric_keys
 xkb_symbols "winkeys" {
     include "ru(common)"
 
